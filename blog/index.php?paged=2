					//Redirect to del.icio.us
					location.href='http://del.icio.us/post?v=2&amp;url='
					+ encodeURIComponent(loc)
					+'&amp;title='
					+encodeURIComponent(document.title);
					//Return false so the link won't be activated. 
					return false;
